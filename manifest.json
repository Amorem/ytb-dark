{
    "name": "Google Chrome Extension Project",
    "version": "1.0.0",
    "description": "Chrome Extension Development & Research",
    "manifest_version": 2,
    "background": {
        "scripts": [
            "background.js"
        ],
        "persistent": false
    },
    "content_scripts": [
        {
            "js": [
                "content.js"
            ],
            "matches": [
                "https://*.youtube.com/*"
            ]
        }
    ],
    "permissions": [
        "bookmarks"
    ]
}